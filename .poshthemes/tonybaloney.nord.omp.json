{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#3B4252",
          "foreground": "#88C0D0",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#3B4252",
          "foreground": "#81A1C1",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#81A1C1",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#81A1C1",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:pistachio-green",
          "template": "\ue718 {{ .Full }} "
        },
        {
          "type": "php",
          "style": "plain",
          "foreground": "p:terminal-blue",
          "template": "\ue73d {{ .Full }} "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:terminal-yellow",
          "template": "\uE235 {{ .Full }}"
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "p:terminal-magenta",
          "template": "\uE624 {{ .Full }}"
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "p:terminal-red",
          "template": "\uE791 {{ .Full}}"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:light-sky-blue",
          "template": "\uFCD1 {{ .Full}}"
        },
        {
          "type": "command",
          "style": "plain",
          "foreground": "p:white-blue",
          "properties": {
            "command": "git log --pretty=format:%cr -1 || date +%H:%M:%S",
            "shell": "bash"
          }
        }
      ],
      "type": "rprompt"
    }
  ],
  "final_space": true,
  "version": 3
}
